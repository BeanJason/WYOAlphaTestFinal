{
  "expo": {
    "name": "WYO App",
    "slug": "WYOApp",
    "owner": "wyoservices",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/notificationIcon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01a1ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.wyo.WYOApp",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey":"AIzaSyAoxuSPmOHBukZIg4Ppf4ZELVs_BR3vDKc"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "NSPhotoLibraryUsageDescription",
          "NSCameraUsageDescription"
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.wyo.WYOApp",
      "versionCode": 1,
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "A"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
